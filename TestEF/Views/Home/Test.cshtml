@model IEnumerable<TestEF.TestTable>
@{
    ViewBag.Title = "Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Test</h2>

<table id="customers" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Birth Date</th>
            <th>Place</th>
        </tr>
        @foreach (var data in Model)
        {
            <tr>
                <td>@data.id</td>
                <td>@data.name</td>
                <td>@data.bdate</td>
                <td>@data.place</td>
            </tr>
        }
    </thead>
    <tbody></tbody>
</table>

